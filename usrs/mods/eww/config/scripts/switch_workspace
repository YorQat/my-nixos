ACTIVE_WORKSPACE=$(~/.config/eww/scripts/active_workspace)

while [[ "$#" -gt 0 ]]; do
    case $1 in
        --up) hyprctl dispatch workspace $(($ACTIVE_WORKSPACE + 1));;
        --down) hyprctl dispatch workspace $(($ACTIVE_WORKSPACE - 1));;
        *) hyprctl dispatch workspace $(($ACTIVE_WORKSPACE + 1));;
    esac
    shift
done